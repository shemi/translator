{
  "name": "shemi/translator",
  "description": "Manage Laravel Translations",
  "keywords": [
    "laravel",
    "translations",
    "translator"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Shemi perez",
      "email": "shemi.perez@gmail.com"
    }
  ],
  "require": {
    "php": "^5.5.9 || ^7.0",
    "illuminate/support": "5.1.* || 5.2.* || 5.3.*",
    "illuminate/translation": "5.1.* || 5.2.* || 5.3.*",
    "illuminate/filesystem": "5.1.* || 5.2.* || 5.3.*",
    "symfony/finder": "^3.0",
    "aza/phpgen": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "Shemi\\Translator\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.2-dev"
    }
  },
  "minimum-stability": "stable"
}
